pipeline {
    agent any
    stages{
        stage ("Git Checkout"){
            steps{
                git 'https://github.com/Mabasha545556/simple-app.git'
            }
        }
        stage ("Maven Build"){
            steps{
            sh 'mvn clean install package'
            }
        }
        stage ("Slack Notification"){
            steps{
            slackSend channel: '#jenkins-slack', color: '#439FE0', message: 'welcome to jenkins slack', tokenCredentialId: 'slack'
            }
        } 
    }
}
